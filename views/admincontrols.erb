	<div style=" color: #333333;
	    text-shadow: 2px 2px 17px #000000; font-family: 'Times New Roman', Times, serif; background-color:#EEEEEE;height:165px;width:165px;border-radius: 5px; border: 1px solid black; margin: 0 auto;  padding: 25px; border-radius: 5px;opacity: 0.8;
  filter: alpha(opacity=80); float: left;">
		<h2>Create account</h2>
	<form action="/admincontrols" method="post">
		Username:<input type = "text" name = "username"><br>
		Password: <input type = "password" name = "password"><br>
		<input type = "submit" value = "Create!">
	</form>
	</div>
	




	<div style="text-align: center; color: #333333;
	    text-shadow: 2px 2px 17px #000000; font-family: 'Times New Roman', Times, serif; background-color:#EEEEEE;height:600px;width:240px;border-radius: 5px;overflow-x: hidden;overflow-y: scroll; border: 1px solid black; margin: 0 auto; display: block; padding: 25px; border-radius: 5px;opacity: 0.8;
  filter: alpha(opacity=80);">

		<h2>Admin controls page</h2>
		<p><b>User list</b></p><br>
		<p>Username - DateJoined - Edit?<br>
			<% @list2.each do |liste| %>
		 <article>
		 <p>
		 <%= liste.username %> - <%= liste.date_joined %> - <%= liste.edit %>
		<form action="/user/<%= liste.username %>" method="post" id="edit">
		<input type="hidden" name="_method" value="put">
		<input type="checkbox" name="edit" <%= "checked" if liste.edit %>>
		 <input type="submit" value="Allow Edit?">
		
		</form>
		

		<form action="/user/delete/<%= liste.username %>" method="post" id="edit">
		 <input type="hidden" name="_method" value="delete">
		 <input type="submit" value="Delete User">
		 </form>

		 
		 <a href="/user/edit/<%= liste.username %>"><input type="submit" value="Edit User"></a>
		 
	

		 </p>
		 </article>
		<% end %> 
	</div>
